{
  "name": "Mock proxy connector",
  "options_page": "options/options.html",
  "browser_action": {
    "default_icon": {
      "19": "images/icon_19.png",
      "38": "images/icon_38.png"
    },
    "default_popup": "popup/popup.html"
  },
  "description": "Connects you to the mock proxy",
  "version": "2.0",
  "devtools_page": "panelCreation/createPanel.html",
  "icons": { "16": "images/icon_16.png",
            "48": "images/icon_48.png",
            "128": "images/icon_128.png" },
  "permissions": [
    "http://*/"
  ],
  "background": {
    "scripts": ["lib/apiConnector.js", "lib/apiBridge.js", "background.js"],
    "css": ["moxy.css"],
    "persistent": false
  },
  "manifest_version": 2
}